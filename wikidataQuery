SELECT ?item ?itemLabel ?geo WHERE {
  ?item wdt:P31 wd:Q27686;
    wdt:P625 ?geo.
  SERVICE wikibase:label { bd:serviceParam wikibase:language "[AUTO_LANGUAGE],en". }
}


https://query.wikidata.org/#SELECT%20%3Fitem%20%3FitemLabel%20%3Fgeo%20WHERE%20%7B%0A%20%20%3Fitem%20wdt%3AP31%20wd%3AQ27686%3B%0A%20%20%20%20wdt%3AP625%20%3Fgeo.%0A%20%20SERVICE%20wikibase%3Alabel%20%7B%20bd%3AserviceParam%20wikibase%3Alanguage%20%22%5BAUTO_LANGUAGE%5D%2Cen%22.%20%7D%0A%7D%0A